<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<servlet>
		<servlet-name>myRegist</servlet-name>
		<servlet-class>web.QuoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myRegist</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>myStocke</servlet-name>
		<servlet-class>web.StockeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myStocke</servlet-name>
		<url-pattern>/stocke</url-pattern>
	</servlet-mapping>
	
	
</web-app>